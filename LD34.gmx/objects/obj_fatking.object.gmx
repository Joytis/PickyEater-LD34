            <string>/// Instantiate Fatness and Poison

fatness  = 0;
poison = 0;

//INIT POSITIONS
x = 528;
y = 480;

bellyScale = 0;
input=0;
purpleness = 0;
loadtime = 180;
hoverAmount= 0;
up = true;

