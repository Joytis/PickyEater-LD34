part_type_orientation(p1, 0, 0, 0, 0, 1);
part_type_blend(p1, 0);
part_type_life(p1, 15, 60);

emitter = part_emitter_create(partSystem);
part_emitter_region(partSystem,
                    emitter,
                    x - 75, 
                    x + 75, 
                    y - 2, 
                    y + 2,
                    ps_shape_ellipse,
                    ps_distr_linear
                    );
